{
  "name": "prerender-docker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"${NODE_ENV}\"",
    "build": "concurrently \"npm run build:server\" \"npm run build:agent\"",
    "build:server": "docker build -t prerender-server ./prerender-server",
    "build:agent": "docker build -t prerender-agent ./prerender-agent",
    "tag": "concurrently \"npm run tag:server\" \"npm run tag:agent\"",
    "tag:server": "docker tag prerender-server 457870823482.dkr.ecr.us-west-2.amazonaws.com/prerender-server",
    "tag:agent": "docker tag prerender-agent 457870823482.dkr.ecr.us-west-2.amazonaws.com/prerender-agent",
    "push": "concurrently \"npm run push:server\" \"npm run push:agent\"",
    "push:server": "docker push 457870823482.dkr.ecr.us-west-2.amazonaws.com/prerender-server",
    "push:agent": "docker push 457870823482.dkr.ecr.us-west-2.amazonaws.com/prerender-agent",
    "start": "docker-compose up",
    "stop": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "node-fetch": "^2.3.0",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1",
    "winston": "^3.1.0"
  }
}
